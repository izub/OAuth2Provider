{
    "name": "izub/oauth2-provider",
    "type": "library",
    "description": "ZF3 OAuth2 Provider module",
    "keywords": [
        "OAuth2",
        "OAuth2 Module",
        "Zend Framework 3",
        "zf3 module"
    ],
    "homepage": "https://github.com/izub/OAuth2Provider",
    "license": "MIT",
    "authors": [
        {
          "name": "Mark Zijlstra",
          "email": "mark@bytecove.com"
        },
        {
            "name": "Franz de Leon",
            "email": "franz_dl@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "bshaffer/oauth2-server-php": "^1.11",
        "zendframework/zend-loader": "^2.6",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-modulemanager": "^2.8",
        "zendframework/zend-servicemanager": "^3.4",
        "zendframework/zend-stdlib": "^3.2",
        "zendframework/zend-view": "^2.11",
        "zendframework/zend-filter": "^2.9",
        "zendframework/zend-http": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-0": {
            "OAuth2Provider": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "autoload-dev" : {
        "psr-0": {
            "OAuth2ProviderTests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
